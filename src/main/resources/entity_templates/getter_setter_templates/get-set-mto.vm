    public ${mtoAssociation.getReferencedEntity()}Impl get${mtoAssociation.getReferencedName()}() {
        return this.${mtoAssociation.getReferencedNameCamelCase()};
    }

    public void set${mtoAssociation.getReferencedName()}(${mtoAssociation.getReferencedEntity()}Impl ${mtoAssociation.getReferencedNameCamelCase()}) {
        this.${mtoAssociation.getReferencedNameCamelCase()} = ${mtoAssociation.getReferencedNameCamelCase()};
    }
